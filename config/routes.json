{
    "blogg/start": {
        "controller": "Default",
        "method": "start"
    },
    "blogg/logout": {
        "login": true,
        "controller": "Admin",
        "method": "logout"
    },
    "blogg/register": {
        "controller": "Posts",
        "method": "register"
    },
    "blogg/newPost": {
        "login": true,
        "controller": "Admin",
        "method": "newPost"
    },
    "blogg/editPost": {
        "login": true,
        "controller": "Admin",
        "method": "editPost"
    },
    "blogg/posts/:page": {
        "controller": "Posts",
        "method": "getAllWithPage",
        "params": {
            "page": "number"
        }
    },
    "blogg/": {
        "controller": "Posts",
        "method": "handleRequest"
    },
    "blogg/posts/search": {
        "controller": "Posts",
        "method": "search"
    },
    "blogg/admin": {
        "login": true,
        "controller": "Admin",
        "method": "getAll"
    },
    "blogg/admin/posts/:id": {
        "controller": "Admin",
        "method": "get",
        "params": {
            "id": "number"
        }
    }
}