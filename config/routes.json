{
    "start": {
        "controller": "Default",
        "method": "start"
    },
    "posts/:page": {
        "controller": "Posts",
        "method": "getAllWithPage",
        "params": {
            "page": "number"
        }
    },
    "posts": {
        "controller": "Posts",
        "method": "getAll"
    },
    "post/:id": {
        "controller": "Posts",
        "method": "get",
        "params": {
            "id": "number"
        }
    },
    "posts/search": {
        "controller": "Posts",
        "method": "search"
    },
    "customers": {
        "controller": "Customer",
        "method": "getAll"
    },
    "customer/:id": {
        "controller": "Customer",
        "method": "get",
        "params": {
            "id": "number"
        }
    }
}