{
    "start": {
        "controller": "Default",
        "method": "start"
    },
    "posts/:page": {
        "controller": "Posts",
        "method": "getAllWithPage",
        "params": {
            "page": "number"
        }
    },
    "posts": {
        "controller": "Posts",
        "method": "getAll"
    },
    "posts/search": {
        "controller": "Posts",
        "method": "search"
    },


    "admin": {
        "controller": "Admin",
        "method": "getAll"
    },
    "admin/posts/:id": {
        "controller": "Admin",
        "method": "get",
        "params": {
            "id": "number"
        }
    }
}